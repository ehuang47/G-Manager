{
  "name": "GManager", 
  "description": "Keeps the emails that spark joy.",
  "version": "1.0", 

  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "index.html", 
    "default_title": "Save that link!" 
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "storage"
  ]
}